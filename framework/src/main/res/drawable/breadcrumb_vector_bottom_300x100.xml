<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="300dp"
    android:height="100dp"
    android:viewportWidth="300"
    android:viewportHeight="100">
    <group
        android:pivotX="0"
        android:pivotY="0"
        android:rotation="180">
        <path
            android:fillType="evenOdd"
            android:pathData="m -300,0 c 40,0 40,-100 100,-100 h 100 c 60,0 40,100 100,100 z"
            android:strokeWidth="2"
            android:strokeColor="#FFFFFFFF"
            android:strokeLineJoin="bevel">
            <aapt:attr name="android:fillColor">
                <gradient
                    android:endX="0"
                    android:endY="0"
                    android:startX="0"
                    android:startY="-100"
                    android:type="linear">
                    <item
                        android:color="#FFFFFFFF"
                        android:offset="0" />
                    <item
                        android:color="@color/breadcrumb_light"
                        android:offset="0.00001" />
                    <item
                        android:color="@color/breadcrumb_dark"
                        android:offset="1" />
                    <!--<item-->
                    <!--android:color="#0000FF00"-->
                    <!--android:offset="0.00001" />-->
                    <!--<item-->
                    <!--android:color="#FF00FF00"-->
                    <!--android:offset="1" />-->
                </gradient>
            </aapt:attr>
        </path>
    </group>
</vector>
