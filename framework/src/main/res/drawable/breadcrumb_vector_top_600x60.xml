                    android:type="linear">
                    <item
                        android:color="#FFFFFFFF"
                        android:offset="0" />
                    <item
                        android:color="@color/breadcrumb_dark"
                        android:offset="0.00001" />
                    <item
                        android:color="@color/breadcrumb_light"
                        android:offset="1" />
                    <!--<item-->
                    <!--android:color="#0000FF00"-->
                    <!--android:offset="0.00001" />-->
                    <!--<item-->
                    <!--android:color="#FF00FF00"-->
                    <!--android:offset="1" />-->

                </gradient>
